{
  "DbContext": {
    "SensitiveDataLoggingEnabled": false,
    "ConnectionResiliencyMaxRetryCount": 10,
    "ConnectionResiliencyMaxRetryDelay": "0.00:00:30",
    "UseAccessToken": true
  },
  "ConnectionStrings": {
    "AppDbContext": "__sqlDbConnectionString__"
  },
  "ApplicationInsights": {
    "ConnectionString": "__applicationInsightsConnectionString__"
  },
  "Serilog": {
    "MinimumLevel": {
      "Default": "Information",
      "Override": {
        "Microsoft": "Warning",
        "Microsoft.EntityFrameworkCore": "Warning",
        "System": "Warning"
      }
    },
    "WriteTo": [
      {
        "Name": "Logger",
        "Args": {
          "configureLogger": {
            "WriteTo": [
              {
                "Name": "Console",
                "Args": {
                  "outputTemplate": "[{Timestamp:HH:mm:ss.fff zzz} {Level:u3}] {Message:lj} <s:{SourceContext}>{NewLine}{Exception}"
                }
              }
            ]
          }
        }
      }
    ]
  },
  "Scheduling": {
    "Host": {
      "quartz.scheduler.instanceName": "Enigmatry.Entry.Scheduler"
    },
    "Jobs": {
      "UpdateProductAmountJob": {
        "Cronex": "__entryBlueprintExampleJobCronExpression__"
      }
    }
  }
}