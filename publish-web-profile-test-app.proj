<?xml version="1.0" encoding="utf-8"?>
<Project ToolsVersion="4.0" xmlns="http://schemas.microsoft.com/developer/msbuild/2003">
  <PropertyGroup>
    <PackageFile>$(MSBuildProjectDirectory)\Artifacts\Enigmatry.Blueprint.App.zip</PackageFile>
    <IisApplicationName>enigmatry-blueprint-test</IisApplicationName>
    <WebDeployEndPoint>https://enigmatry-blueprint-test.scm.azurewebsites.net/msdeploy.axd</WebDeployEndPoint>
    <GenerateSetParameters>true</GenerateSetParameters>
    <AuthType>Basic</AuthType>
    <CredentialName>enigmatry_blueprint_test</CredentialName>
  </PropertyGroup>
  <ItemGroup>
    <ParameterValue Include="API Uri">
      <Value>/api</Value>
    </ParameterValue>
  </ItemGroup>
</Project>