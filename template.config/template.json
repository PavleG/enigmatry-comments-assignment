{
    "$schema": "http://json.schemastore.org/template",
    "author": "eko.",
    "classifications": [ "Blueprint", ".netcore", "Angular", "web" ],
    "identity": "Enigmatry.Blueprint.Template",
    "name": "Enigmatry Blueprint Project",
    "shortName": "blueprint",
    "tags": {
      "language": "C#",
      "type": "project"
    },
    "sourceName": "Enigmatry.Blueprint",
    "preferNameDirectory": true,
    "symbols": {
        "name": {
        "type": "parameter",
        "defaultValue": "Enigmatry.Blueprint",
        "replaces": "Enigmatry.Blueprint"
        },
        "appProjectName": {
          "type": "parameter",
          "replaces": "enigmatry-blueprint",
          "FileRename": "enigmatry-blueprint",
          "description": "the kebab-cased name (i.e. enigmatry-blueprint)"
        },
        "contextName": {
          "type": "parameter",
          "replaces": "BlueprintContext",
          "FileRename": "BlueprintContext",
          "description": "The DbContext name, PascalCased"
        }
    }
  }