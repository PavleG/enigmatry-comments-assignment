{
  "UseDeveloperExceptionPage": true,
  "SensitiveDataLoggingEnabled": true, 
  "ConnectionStrings": {
    "BlueprintContext": "Server=.;Database=Blueprint-Core;Trusted_Connection=True;MultipleActiveResultSets=true"
  },
  "App": {
    "Localization": {
      "CacheDuration":  "0.00:00:15"
    }
  },
  "Serilog": {
    "MinimumLevel": {
      "Default": "Debug"
    },
    "WriteTo":  [
      {
        "Name": "RollingFile",
        "Args": {
          "pathFormat": "c:/temp/logs/enigmatry-blueprint-api-{Date}.log"
        }
      }
    ]
  },
  "ApplicationInsights": {
    "InstrumentationKey": ""
  }
}
