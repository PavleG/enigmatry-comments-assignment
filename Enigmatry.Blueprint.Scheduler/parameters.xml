<?xml version="1.0" encoding="utf-8" ?>
<parameters>
  <parameter name="Service Name" description="Service Name" defaultValue="Enigmatry.Blueprint.Service">
      <parameterEntry kind="TextFile" scope="appsettings.json" match="__serviceName__" />
  </parameter>
  <parameter name="Service Display Name" description="Service Display Name" defaultValue="Enigmatry Blueprint Service">
      <parameterEntry kind="TextFile" scope="appsettings.json" match="__serviceDisplayName__" />
  </parameter>
  <parameter name="Service Description" description="Service Description" defaultValue="Enigmatry Blueprint Service Description">
      <parameterEntry kind="TextFile" scope="appsettings.json" match="__serviceDescription__" />
  </parameter>
  <parameter name="Test Job Scheduler Interval" description="Test Job Scheduler Interval" defaultValue="0 0/15 * * * ? *">
    <parameterEntry kind="TextFile" scope="appsettings.json" match="__testJobSchedulerInterval__" />
  </parameter>
  <parameter name="Test Job Enabled" description="Test Job Enabled" defaultValue="true">
    <parameterEntry kind="TextFile" scope="appsettings.json" match="__testJobEnabled__" />
  </parameter>
</parameters>