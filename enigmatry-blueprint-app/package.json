{
  "name": "enigmatry-blueprint-app",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "test": "ng test",
    "lint": "npx ng lint && npm run lint:styles",
    "lint:styles": "stylelint --fix src/**/*.scss",
    "e2e": "ng e2e",
    "audit:ci": "npm audit --json|npm-audit-reporter-teamcity",
    "nswag": "node_modules/.bin/nswag run nswag.json",
    "codegen": "enigmatry-codegen --source-assembly ../Enigmatry.Blueprint.CodeGeneration.Setup/bin/Debug/net5/Enigmatry.Blueprint.CodeGeneration.Setup.dll --destination-directory ./src/app/features",
    "codegen:install": "dotnet tool install enigmatry.codegeneration.console -g",
    "codegen:update": "dotnet tool update enigmatry.codegeneration.console -g"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^12.1.3",
    "@angular/cdk": "^12.1.3",
    "@angular/common": "^12.1.3",
    "@angular/compiler": "^12.1.3",
    "@angular/core": "^12.1.3",
    "@angular/forms": "^12.1.3",
    "@angular/material": "^12.1.3",
    "@angular/platform-browser": "^12.1.3",
    "@angular/platform-browser-dynamic": "^12.1.3",
    "@angular/router": "^12.1.3",
    "@ngx-formly/core": "^5.10.22",
    "@ngx-formly/material": "^5.10.22",
    "nswag": "^13.12.0",
    "rxjs": "^6.6.7",
    "tslib": "^2.3.0",
    "zone.js": "~0.11.4"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^12.1.3",
    "@angular-eslint/builder": "^12.3.1",
    "@angular-eslint/eslint-plugin": "^12.3.1",
    "@angular-eslint/eslint-plugin-template": "^12.3.1",
    "@angular-eslint/schematics": "^12.3.1",
    "@angular-eslint/template-parser": "^12.3.1",
    "@angular/cli": "^12.1.3",
    "@angular/compiler-cli": "^12.1.3",
    "@angular/localize": "^12.1.3",
    "@types/jasmine": "^3.8.1",
    "@types/node": "^16.4.0",
    "@typescript-eslint/eslint-plugin": "4.28.4",
    "@typescript-eslint/parser": "4.28.4",
    "eslint": "^7.31.0",
    "eslint-plugin-import": "2.23.4",
    "eslint-plugin-jsdoc": "35.5.1",
    "eslint-plugin-prefer-arrow": "1.2.3",
    "jasmine-core": "~3.8.0",
    "jasmine-spec-reporter": "~7.0.0",
    "karma": "~6.3.4",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage": "~2.0.3",
    "karma-jasmine": "~4.0.1",
    "karma-jasmine-html-reporter": "^1.7.0",
    "npm-audit-reporter-teamcity": "0.3.1",
    "protractor": "~7.0.0",
    "stylelint": "13.13.1",
    "stylelint-config-property-sort-order-smacss": "7.1.0",
    "stylelint-scss": "3.20.1",
    "stylelint-declaration-block-no-ignored-properties": "2.4.0",
    "stylelint-group-selectors": "1.0.8",
    "stylelint-max-lines": "0.1.0",
    "stylelint-selector-no-empty": "1.0.8",
    "stylelint-use-nesting": "3.0.0",
    "ts-node": "~10.1.0",
    "typescript": "~4.2.4"
  }
}