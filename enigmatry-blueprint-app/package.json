{
  "name": "enigmatry-blueprint-app",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "test": "ng test",
    "lint": "npm run lint:ts && npm run lint:styles",
    "lint:ts": "npx ng lint --fix",
    "lint:styles": "stylelint --fix src/**/*.scss",
    "e2e": "ng e2e",
    "audit:ci": "npm audit --json|npm-audit-reporter-teamcity",
    "nswag": "node_modules\\.bin\\nswag run nswag.json",
    "codegen:run": "entry-codegen --source-assembly ../Enigmatry.Blueprint.CodeGeneration.Setup/bin/Debug/net7.0/Enigmatry.Blueprint.CodeGeneration.Setup.dll --destination-directory ./src/app/features",
    "codegen:run-i18n": "entry-codegen --source-assembly ../Enigmatry.Blueprint.CodeGeneration.Setup/bin/Debug/net7.0/Enigmatry.Blueprint.CodeGeneration.Setup.dll --destination-directory ./src/app/features --enable-i18n true",
    "codegen:install": "dotnet tool install enigmatry.entry.codegeneration.tools -g",
    "codegen:update": "dotnet tool update enigmatry.entry.codegeneration.tools -g",
    "i18n": "ng extract-i18n --format json --output-path src/assets/locale --out-file messages.en.json"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^15.2.9",
    "@angular/cdk": "^15.2.9",
    "@angular/common": "^15.2.9",
    "@angular/compiler": "^15.2.9",
    "@angular/core": "^15.2.9",
    "@angular/forms": "^15.2.9",
    "@angular/material": "^15.2.9",
    "@angular/platform-browser": "^15.2.9",
    "@angular/platform-browser-dynamic": "^15.2.9",
    "@angular/router": "^15.2.9",
    "@azure/msal-browser": "^3.1.0",
    "@enigmatry/entry-components": "1.15.1-preview.29",
    "@enigmatry/entry-form": "1.15.1-preview.29",
    "@enigmatry/entry-table": "1.15.1-preview.29",
    "@ngx-formly/core": "6.1.7",
    "@ngx-formly/material": "6.1.7",
    "rxjs": "^7.8.1",
    "tslib": "^2.3.0",
    "zone.js": "~0.11.4"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^15.2.9",
    "@angular/cli": "^15.2.9",
    "@angular/compiler-cli": "^15.2.9",
    "@angular/localize": "^15.2.9",
    "@enigmatry/eslint-config": "1.15.1-preview.29",
    "@enigmatry/scss-foundation": "1.15.1-preview.29",
    "@enigmatry/stylelint-config": "1.15.1-preview.29",
    "@types/jasmine": "^3.8.1",
    "@types/node": "^16.4.3",
    "jasmine-core": "~3.8.0",
    "jasmine-spec-reporter": "~7.0.0",
    "karma": "~6.3.4",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage": "^2.1.0",
    "karma-jasmine": "~4.0.1",
    "karma-jasmine-html-reporter": "^1.7.0",
    "npm-audit-reporter-teamcity": "0.3.1",
    "nswag": "^13.16.1",
    "protractor": "~7.0.0",
    "ts-node": "~10.4.0",
    "typescript": "4.9.5"
  }
}
