{
  "name": "enigmatry-blueprint-app",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "test": "ng test",
    "lint": "npm run lint:ts && npm run lint:styles",
    "lint:ts": "npx ng lint --fix",
    "lint:styles": "stylelint --fix src/**/*.scss",
    "e2e": "ng e2e",
    "audit:ci": "npm audit --json|npm-audit-reporter-teamcity",
    "nswag": "node_modules/.bin/nswag run nswag.json",
    "codegen": "enigmatry-codegen --source-assembly ../Enigmatry.Blueprint.CodeGeneration.Setup/bin/Debug/net5/Enigmatry.Blueprint.CodeGeneration.Setup.dll --destination-directory ./src/app/features",
    "codegen:i18n": "enigmatry-codegen --source-assembly ../Enigmatry.Blueprint.CodeGeneration.Setup/bin/Debug/net5/Enigmatry.Blueprint.CodeGeneration.Setup.dll --destination-directory ./src/app/features --enable-i18n true",
    "codegen:install": "dotnet tool install enigmatry.codegeneration.console -g",
    "codegen:update": "dotnet tool update enigmatry.codegeneration.console -g",
    "i18n": "ng extract-i18n --format json --output-path src/assets/locale --out-file messages.en.json"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^12.1.3",
    "@angular/cdk": "^12.1.3",
    "@angular/common": "^12.1.3",
    "@angular/compiler": "^12.1.3",
    "@angular/core": "^12.1.3",
    "@angular/forms": "^12.1.3",
    "@angular/material": "^12.1.3",
    "@angular/platform-browser": "^12.1.3",
    "@angular/platform-browser-dynamic": "^12.1.3",
    "@angular/router": "^12.1.3",
    "@enigmatry/angular-building-blocks": "^1.0.93",
    "@enigmatry/formly-extensions": "^1.0.93",
    "@ngx-formly/core": "^5.10.22",
    "@ngx-formly/material": "^5.10.22",
    "nswag": "^13.12.1",
    "rxjs": "^6.6.7",
    "tslib": "^2.3.0",
    "zone.js": "~0.11.4"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^12.1.3",
    "@angular/cli": "^12.1.3",
    "@angular/compiler-cli": "^12.1.3",
    "@angular/localize": "^12.1.3",
    "@types/jasmine": "^3.8.1",
    "@types/node": "^16.4.3",
    "eslint": "^7.32.0",
    "@enigmatry/eslint-config": "^1.0.0",
    "jasmine-core": "~3.8.0",
    "jasmine-spec-reporter": "~7.0.0",
    "karma": "~6.3.4",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage": "~2.0.3",
    "karma-jasmine": "~4.0.1",
    "karma-jasmine-html-reporter": "^1.7.0",
    "npm-audit-reporter-teamcity": "0.3.1",
    "protractor": "~7.0.0",
    "stylelint": "13.13.1",
    "@enigmatry/stylelint-config": "^1.0.0",
    "ts-node": "~10.1.0",
    "typescript": "4.3.5"
  }
}
